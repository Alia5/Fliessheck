{
    "name": "fliessheck",
    "version": "1.0.0",
    "license": "Apache-2.0",
    "author": {
        "name": "Peter Repukat - FlatspotSoftware",
        "email": "peter@flatspot.pictures"
    },
    "type": "module",
    "types": "./dist/index.d.ts",
    "main": "./dist.cjs/index.js",
    "module": "./dist/index.js",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist.cjs/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "lint": "eslint src/",
        "lint:fix": "eslint src/ --fix",
        "test": "jest",
        "clean": "rimraf dist dist.cjs",
        "build:types": "tsc -p tsconfig.json --outDir dist.cjs --emitDeclarationOnly && tsc -p tsconfig.json --emitDeclarationOnly",
        "build": "rollup -c && pnpm run build:types",
        "dev": "rollup -c --watch --watch.onEnd=\"pnpm run build:types\""
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^25.0.3",
        "@rollup/plugin-json": "^6.0.0",
        "@rollup/plugin-node-resolve": "^15.1.0",
        "@types/cors": "^2.8.13",
        "@types/express": "^4.17.17",
        "@types/jest": "^29.5.3",
        "@types/node": "^20.4.9",
        "@typescript-eslint/eslint-plugin": "^6.3.0",
        "@typescript-eslint/parser": "^6.3.0",
        "axios": "^1.4.0",
        "concurrently": "^8.2.0",
        "esbuild": "^0.19.0",
        "eslint": "^8.46.0",
        "eslint-plugin-import": "^2.28.0",
        "eslint-plugin-no-null": "^1.0.2",
        "eslint-plugin-prefer-arrow": "^1.2.3",
        "eslint-plugin-tsc": "^2.0.0",
        "eslint-plugin-tsdoc": "^0.2.17",
        "jest": "^29.6.2",
        "nodemon": "^3.0.1",
        "rimraf": "^5.0.1",
        "rollup": "^3.28.0",
        "rollup-plugin-esbuild": "^5.0.0",
        "ts-jest": "^29.1.1",
        "tsc-watch": "^6.0.4",
        "typescript": "^5.1.6"
    },
    "dependencies": {
        "@types/jsonwebtoken": "^9.0.2",
        "jsonwebtoken": "^9.0.1",
        "log4js": "^6.9.1"
    },
    "peerDependencies": {
        "cors": "^2.8.5",
        "express": "^4.18.2",
        "socket.io": "^4.7.2"
    }
}
