{
    "name": "fliessheck",
    "version": "1.0.0",
    "license": "Apache-2.0",
    "author": {
        "name": "Peter Repukat - FlatspotSoftware",
        "email": "peter@flatspot.pictures"
    },
    "type": "module",
    "types": "./dist/index.d.ts",
    "main": "./dist.cjs/index.js",
    "module": "./dist/index.js",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist.cjs/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "lint": "eslint src/",
        "lint:fix": "eslint src/ --fix",
        "lint:ci": "eslint src/ --no-fix --format=compact",
        "test": "jest",
        "clean": "rimraf dist dist.cjs",
        "build:types": "concurrently 'tsc -p tsconfig.json --outDir dist.cjs --emitDeclarationOnly --declaration' 'tsc -p tsconfig.json --outDir dist --emitDeclarationOnly --declaration'",
        "build": "rollup -c && pnpm run build:types",
        "dev": "rollup -c --watch --watch.onEnd=\"pnpm run build:types\""
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3.0.2",
        "@rollup/plugin-commonjs": "^25.0.7",
        "@rollup/plugin-json": "^6.1.0",
        "@rollup/plugin-node-resolve": "^15.2.3",
        "@rollup/plugin-typescript": "^11.1.6",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/jest": "^29.5.12",
        "@types/node": "^20.12.11",
        "@types/socket.io": "^3.0.2",
        "axios": "^1.6.8",
        "concurrently": "^8.2.2",
        "cors": "^2.8.5",
        "esbuild": "^0.21.1",
        "eslint": "^9.2.0",
        "eslint-formatter-compact": "^8.40.0",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-no-null": "^1.0.2",
        "eslint-plugin-prefer-arrow": "^1.2.3",
        "eslint-plugin-tsc": "^2.0.0",
        "eslint-plugin-tsdoc": "^0.2.17",
        "express": "^4.19.2",
        "globals": "^15.1.0",
        "jest": "^29.7.0",
        "nodemon": "^3.1.0",
        "rimraf": "^5.0.5",
        "rollup": "^4.17.2",
        "socket.io": "^4.7.5",
        "ts-jest": "^29.1.2",
        "tsc-watch": "^6.2.0",
        "typescript": "^5.4.5",
        "typescript-eslint": "^7.8.0"
    },
    "dependencies": {
        "@types/jsonwebtoken": "^9.0.2",
        "jsonwebtoken": "^9.0.1",
        "log4js": "^6.9.1"
    },
    "peerDependencies": {
        "cors": "^2.8.5",
        "express": "^4.18.2",
        "socket.io": "^4.7.2"
    }
}
